{
  "name": "gas-esbuild-temp",
  "version": "1.0.0",
  "type": "module",
  "description": "bundle for gas",
  "author": "yoshikoro",
  "license": "ISC",
  "dependencies": {
    "@types/google-apps-script": "^1.0.84"
  },
  "scripts": {
    "build": "node esbuild.js",
    "nbuild": "tsc",
    "push": "clasp push",
    "open": "clasp open",
    "cp": "cp ./src/appsscript.json ./dist/appsscript.json",
    "cpw": "cp ./src/static/* ./dist/",
    "nodecp": "node -e \"require('fs').cpSync('./src/appsscript.json','./dist/appsscript.json',{recursive:true})\"",
    "nodecpw": "node -e \"require('fs').cpSync('./src/static/', './dist/', {recursive: true})\"",
    "adeploy": "npm run cp && npm run build && npm run push",
    "deploy": "npm run build && npm run push",
    "ndeploy": "npm run nbuild && npm run push",
    "clearn": "rm -f ./dist/*"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "esbuild": "^0.24.0",
    "esbuild-gas-plugin": "^0.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
